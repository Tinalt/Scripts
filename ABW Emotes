local kavoUi = loadstring(game:HttpGet("https://raw.githubusercontent.com/Toilatin/Insane-elevator/main/Insane2"))()
local window = kavoUi.CreateLib("Free Emotes (ABW)","BloodTheme")

---Hi

local Tab1 = window:NewTab("Main")
local Tab1Section = Tab1:NewSection("Emotes")

--Buttons

Tab1Section:NewButton("Gaster","Free",function()

-- Create a new Animation object
local animation = Instance.new("Animation")
animation.AnimationId = "rbxassetid://14054114234"

-- Function to play the animation
local function playAnimation()
    -- Make sure the script is running on a player
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")
    
    -- Load the animation to the humanoid
    local animationTrack = humanoid:LoadAnimation(animation)
    
    -- Play the animation
    animationTrack:Play()
end

-- Call the function to play the animation
playAnimation()
end)

